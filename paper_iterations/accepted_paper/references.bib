	journal = {Biological Psychiatry: Cognitive Neuroscience and Neuroimaging},
	author = {Meda, Shashwath A. and Hawkins, Keith A. and Dager, Alecia D. and Tennen, Howard and Khadka, Sabin and Austad, Carol S. and Wood, Rebecca M. and Raskin, Sarah and Fallahi, Carolyn R. and Pearlson, Godfrey D.},
	month = jul,
