	pages = {1--15},
	file = {Full Text PDF:/Users/michaeltopper/Zotero/storage/GMV3ZL37/Hayes and Boyle - 2020 - Identity and Hooking up at a U.S. “Party School”.pdf:application/pdf;Snapshot:/Users/michaeltopper/Zotero/storage/KPEWVKBC/01639625.2020.html:text/html},
}

